<% for (var i=(page * page_num) - page_num; i < (page * page_num); i++) { if (i> length) {
    i++;
} else {
    var data = liquors[i];
    <!-- 별 내가 조진다 -->
    var liquor_score = data.liquor_score;
    var liquor_score_star;
    var result;
    result = liquor_score % 1;
    <!-- 조져지는건 나였다 현재 별점 4.5만 동작함 3.5 나오면 난리남-->
%>
<div class="drink-all">
    <ul>
        <li class="drink">
            <div class="drink-num">
                <%= i +1 %>
            </div>
            <div class="drink-photo">
                <img src="<%= data.liquor_img %>">
            </div>
            <div class="drink-content">
                <div class="drink-name">
                    <%= data.liquor_name %>
                </div>
                <div class="drink-raw">
                    <%= data.liquor_raw2 %>
                </div>
